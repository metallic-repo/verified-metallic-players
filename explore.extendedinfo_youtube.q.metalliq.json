 "priority"     : 700,
 "id"           : "explore.extendedinfo_youtube.q",
 "filters"      : {},
 "postprocess"  : "",
 "movies"       : [
  [
   {
    "link"      : "plugin://script.extendedinfo/?info=youtubesearch&id={name}",
    "steps"     : [
     "{title}.*{year}.*"
     ],
    "action"    : "ACTIVATE"
   },
   {
    "link"      : "plugin://script.extendedinfo/?info=youtubesearch&id={title}",
    "steps"     : [
     "{title}.*"
     ],
    "action"    : "PLAY"
   }
  ]
 ],
 "tvshows"      : [
  [
   {
